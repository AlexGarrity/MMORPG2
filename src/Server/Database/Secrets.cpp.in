#include "Database/Secrets.hpp"

namespace Server::Database
{

	auto getURI() -> std::string
	{
		return "${SERVER_DATABASE_URI}";
	}

	auto getConnectionString() -> std::string
	{
		return "${SERVER_DATABASE_CONNECTION_STRING}";
	}

	auto getUsername() -> std::string
	{
		return "${SERVER_DATABASE_USERNAME}";
	}

	auto getPassword() -> std::string
	{
		return "${SERVER_DATABASE_PASSWORD}";
	}

} // namespace Server::Database