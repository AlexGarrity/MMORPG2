#include "Version.hpp"

namespace Server::Version
{

	auto getMajor() -> std::uint32_t
	{
		return ${SERVER_VERSION_MAJOR} + 0;
	}

	auto getMinor() -> std::uint32_t
	{
		return ${SERVER_VERSION_MINOR} + 0;
	}

	auto getPatch() -> std::uint32_t
	{
		return ${SERVER_VERSION_PATCH} + 0;
	}

	auto getCommit() -> std::uint32_t
	{
		return ${SERVER_VERSION_COMMIT} + 0;
	}

} // namespace Server::Version